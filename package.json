  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "build": "npm run build:webpack && npm run build:electron:main",
    "build:webpack": "copyfiles .env dist/ && webpack --display-error-details",
    "build:electron:main": "tsc main.ts --outDir dist && copyfiles package.json dist && cd dist && npm install --prod && cd ..",
    "electron:dev": "npm run build && electron ./dist",
    "start:web": "ng serve",
    "test": "rimraf coverage && karma start ./karma.conf.js"
  },
